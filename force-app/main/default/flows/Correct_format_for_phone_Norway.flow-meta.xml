<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <description>Phone number of contact in correct format</description>
        <name>Contact_Phone</name>
        <label>Contact Phone</label>
        <locationX>176</locationX>
        <locationY>311</locationY>
        <assignmentItems>
            <assignToReference>$Record.Phone</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Formula_Phone</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>$Record.MobilePhone</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Formula_Mobile</elementReference>
            </value>
        </assignmentItems>
    </assignments>
    <description>Flow to use correct format for contact&apos;s phone field for Norway. Remove all blank spaces.</description>
    <environments>Default</environments>
    <formulas>
        <name>Formula_Mobile</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!$Record.MobilePhone}, &quot; &quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>Formula_Phone</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!$Record.Phone}, &quot; &quot;, &quot;&quot;)</expression>
    </formulas>
    <interviewLabel>Correct format for phone- Norway {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Correct format for phone- Norway</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Contact_Phone</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Country__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Norway</stringValue>
            </value>
        </filters>
        <object>Contact</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
    <variables>
        <name>recordId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
</Flow>
