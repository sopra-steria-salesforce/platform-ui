<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Region__c</fullName>
    <description>Fetches data from the &quot;Region__c)&quot; field on Lead or Contact</description>
    <externalId>false</externalId>
    <formula>IF(
    ISPICKVAL(Lead.Region__c, &quot;&quot;), 
    TEXT(Contact.Region__c), 
    TEXT(Lead.Region__c)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Region</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
