<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OwnedByUser__c</fullName>
    <description>Used to filter only my campaign memebers</description>
    <externalId>false</externalId>
    <formula>IF((Contact.OwnerId = $User.Id || Lead.OwnerId = $User.Id), 1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>OwnedBy$User</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
