    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Entry_to_correct_record_type</name>
        <label>Entry to correct record type</label>
        <locationX>182</locationX>
        <locationY>539</locationY>
        <defaultConnectorLabel>Other -&gt; Exit</defaultConnectorLabel>
        <rules>
            <name>Pardot</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Campaign.RecordType.Id</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Get_Campaign_Record_Type.Id</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Campaign.Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Seminar</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Contacts</targetReference>
            </connector>
            <label>Pardot</label>
        </rules>
    </decisions>
    <decisions>
        <name>Status_Regsitered</name>
        <label>Status Regsitered</label>
        <locationX>380</locationX>
        <locationY>323</locationY>
        <defaultConnector>
            <targetReference>Update_Member_Status_Change_Date</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Other</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Registered</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ContactId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Campaign_Record_Type</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>Every time a contact attends an event an Activity will be created with the Start date of the event and added to the contact.
Excluded campaigns from Sweden.</description>
    <environments>Default</environments>
    <formulas>
        <name>DescriptionField</name>
        <dataType>String</dataType>
        <expression>&quot;Kontakten har meldt seg p√• eventet: &quot; &amp;  {!$Record.Campaign.Name}</expression>
    </formulas>
    <interviewLabel>Record Trigger Flow: Create Event when Contact Attends Event {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Record Trigger Flow: Create Event when Contact Attends Event</label>
    <processMetadataValues>
        <name>BuilderType</name>
