<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Business_Unit_Count__c</fullName>
    <externalId>false</externalId>
    <formula>IF(INCLUDES(Business_Unit__c, &quot;APPS&quot;), 1, 0 ) + 
IF(INCLUDES(Business_Unit__c, &quot;Advisory&quot;), 1, 0 ) + 
IF(INCLUDES(Business_Unit__c, &quot;DPS&quot; ) , 1, 0) +
IF(INCLUDES(Business_Unit__c, &quot;BD&quot; ) , 1, 0) +
IF(INCLUDES(Business_Unit__c, &quot;Biometrics&quot; ) , 1, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Business Unit Count</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
