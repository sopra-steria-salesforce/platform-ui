<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Close_Date_Overdue__c</fullName>
    <description>Gives the insight if the Close Date of an Opportunity has passed or is still ongoing.</description>
    <externalId>false</externalId>
    <formula>IF( CloseDate &lt; TODAY(), &quot;Overdue&quot;, &quot;&quot;)</formula>
    <label>Close Date Overdue</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
