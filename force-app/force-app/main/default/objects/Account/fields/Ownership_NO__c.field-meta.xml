<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ownership_NO__c</fullName>
    <description>Used for weekly forecast reporting to FR. After we modified these 4 values in Sector picklist, France wants to keep old values for consistency</description>
    <externalId>false</externalId>
    <formula>CASE(Ownership, 
					&apos;FS&apos;, &apos;Finance&apos;,
     &apos;Healthcare&apos; , &apos;Health&apos;,
     &apos;TMT&apos;, &apos;Telecom &amp; Media&apos;,
					TEXT(Ownership))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>SectorNO</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
